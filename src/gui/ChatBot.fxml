<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="620.0" prefWidth="948.0" style="-fx-background-color: #20120F;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChatBotController">
    <children>
        <Label fx:id="lblTitle" layoutX="14.0" layoutY="14.0" text="Chat Bot" textFill="#f7f3f3">
            <font>
                <Font name="System Bold" size="39.0" />
            </font>
        </Label>
        <TextField fx:id="txtInput" layoutX="31.0" layoutY="198.0" prefHeight="56.0" prefWidth="338.0" promptText="You have any question?" />
        <Button fx:id="btnSend" layoutX="404.0" layoutY="213.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="186.0" style="-fx-background-color: #8D4004;" text="Send" textFill="WHITE" />
        <Label fx:id="lblOutput" alignment="TOP_LEFT" layoutX="31.0" layoutY="272.0" prefHeight="122.0" prefWidth="654.0" style="-fx-background-color: white;" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="19.0" />
            </font>
        </Label>
      <Button layoutX="747.0" layoutY="561.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="186.0" style="-fx-background-color: #8D4004; -fx-text-fill: white;" text="Back" />
    </children>
</AnchorPane>
